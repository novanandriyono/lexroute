{
    "name": "novanandriyono/lexroute",
    "description": "Extra command route for laravel",
    "homepage": "https://github.com/novanandriyono/lexroute",
    "license": "MIT",
    "keywords": [
        "laravel",
        "lexroute"
    ],
    "authors": [
        {
            "name": "Novan Andriyono",
            "email": "evelynradtya@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.6.*",
        "novanandriyono/dpscan": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Lexroute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lexroute\\LexrouteServiceProvider"
            ]
        },
        "branch-alias": {
                "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
